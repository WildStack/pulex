{
  "description": "React UI library integrated with mobx",
  "author": "Giorgi Kumelashvili <https://github.com/GiorgiKumelashvili>",
  "license": "MIT",
  "name": "@pulexui/core",
  "version": "0.0.17",
  "type": "module",
  "main": "dist/main.cjs",
  "types": "dist/main.d.ts",
  "module": "dist/main.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WildStack/pulex.git"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "start": "vite --open",
    "dev": "vite --open",
    "build": "tsc --p ./tsconfig-build.json && npm run lint && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "publish:npm": "npm publish --access public",
    "npm:link:mac": "cd ./lib && npm link && cd ../ && npm link @pulexui/core"
  },
  "dependencies": {
    "mobx": "^6.12.1",
    "mobx-react-lite": "^4.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "mobx": "^6.12.1",
    "mobx-react-lite": "^4.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-dts": "^3.8.0",
    "vite-plugin-lib-inject-css": "^2.0.1"
  }
}
